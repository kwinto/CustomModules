  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Cognigy/CustomModules/issues"
  },
  "homepage": "https://github.com/Cognigy/CustomModules#readme",
  "dependencies": {
    "nodemailer": "^6.3.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.5",
    "tslint": "^5.16.0",
    "typescript": "^3.1.6"
  }
}
